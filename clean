file="config"
  while read -r line; do
      language="$line"
  done < "$file"

  if [ $( docker ps -a | grep ${language}-proj | wc -l ) -gt 0 ]; then
    echo "removing ..."
    docker stop ${language}-proj && docker rm ${language}-proj 
  else
    echo "No docker containers"
  fi

  if [ $( docker images | grep ${language}-sandbox | wc -l ) -gt 0 ]; then
    docker rmi ${language}-sandbox 
  else
    echo "No docker images"
  fi

  rm config

